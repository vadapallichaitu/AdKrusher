{
    "manifest_version": 2,
    "name": "AdKrusher",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    
    "description": "crushes google ads",
    "version": "1.0.0",
    "icons": {
        "128": "128x128.png"
    },
    "browser_action": {
        "default_icon": "16x16.png",
        "default_title": "AdKrusher"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "https://www.google.com/*",
        "https://www.google.co.in/*",
        "https://www.youtube.com/*",
        "https://www.medium.com/*",
        "https://medium.com/*"
    ]
}
